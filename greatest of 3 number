#include <stdio.h>

int main() {
    int a,b,c;
    printf("enter 3 numbers");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b&&a>c)
    {
        printf("greatest no is %d",a);}
        else if(b>a&&b>c)
       {  printf("greatest no is %d",b);}
         else
         { printf("greatest no is %d",c);}
        
    
    
    return 0;
}

